{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@babel/core": "7.0.0",
    "@emotion/react": "11.9.0",
    "@mantine/core": "5.2.4",
    "@mantine/dates": "5.2.4",
    "@mantine/hooks": "5.2.4",
    "@mantine/next": "^5.2.4",
    "@mantine/notifications": "^5.2.4",
    "@tabler/icons": "^1.90.0",
    "dayjs": "^1.11.4",
    "dexie": "^3.2.2",
    "dexie-react-hooks": "^1.1.1",
    "next": "12.2.5",
    "react": "17.0",
    "react-dom": "17.0",
    "react-hook-form": "^7.34.2"
  },
  "devDependencies": {
    "@types/node": "18.0.6",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "eslint": "8.20.0",
    "eslint-config-next": "12.2.3",
    "typescript": "4.7.4"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 2020,
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "extends": [
      "plugin:@typescript-eslint/recommended",
      "plugin:prettier/recommended",
      "plugin:react/recommended",
      "plugin:react-hooks/recommended"
    ],
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "plugins": [
      "react-hooks"
    ],
    "rules": {
      "@typescript-eslint/explicit-module-boundary-types": "off",
      "react/display-name": "off",
      "react/prop-types": "off",
      "prettier/prettier": [
        "error",
        {
          "endOfLine": "auto"
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules",
    "dist"
  ]
}
